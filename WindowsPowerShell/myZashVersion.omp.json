{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "#E36464",
          "properties": {
            "user_info_separator": "",
            "display_host": false,
            "prefix": ""
          }
        },
        {
          "type": "text",
          "style": "plain",
          "foreground": "#62ED8B",
          "properties": {
            "text": "\u279C",
            "postfix": "",
            "prefix": ""
          }
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "#56B6C2",
          "properties": {
            "style": "full",
            "postfix": ""
          }
        },
        {
          "type": "poshgit",
          "style": "powerline"
        },
        {
          "type": "exit",
          "style": "plain",
          "foreground": "#df5353",
          "properties": {
            "prefix": " \uF119",
            "display_exit_code": false,
            "postfix": ""
          }
        }
      ]
    }
  ],
  "final_space": true
}
